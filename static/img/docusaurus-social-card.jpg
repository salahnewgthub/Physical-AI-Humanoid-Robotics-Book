<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1200 630">
  <rect width="1200" height="630" fill="#f8f9fa"/>

  <!-- Title -->
  <text x="600" y="200" font-size="48" text-anchor="middle" fill="#222" font-family="Arial">Physical AI & Humanoid Robotics</text>

  <!-- Subtitle -->
  <text x="600" y="280" font-size="24" text-anchor="middle" fill="#444" font-family="Arial">Connecting AI Intelligence to Humanoid Robot Bodies</text>

  <!-- Robot/AI graphic -->
  <g transform="translate(600, 420)">
    <!-- Robot body -->
    <rect x="-50" y="-30" width="100" height="60" rx="10" fill="#4a90e2" opacity="0.8"/>

    <!-- Robot head -->
    <circle cx="0" cy="-60" r="30" fill="#5fa8d3" opacity="0.8"/>

    <!-- Eyes -->
    <circle cx="-12" cy="-65" r="5" fill="#fff"/>
    <circle cx="12" cy="-65" r="5" fill="#fff"/>
    <circle cx="-12" cy="-65" r="2" fill="#000"/>
    <circle cx="12" cy="-65" r="2" fill="#000"/>

    <!-- Circuit pattern -->
    <path d="M-40,0 L-20,-10 L0,0 L20,-10 L40,0" stroke="#000" stroke-width="2" fill="none"/>

    <!-- AI symbol -->
    <circle cx="-20" cy="20" r="15" stroke="#000" stroke-width="2" fill="none"/>
    <path d="M-28,12 L-12,28 M-28,28 L-12,12" stroke="#000" stroke-width="2"/>
  </g>

  <!-- Footer -->
  <text x="600" y="600" font-size="18" text-anchor="middle" fill="#666" font-family="Arial">Physical AI & Humanoid Robotics Book</text>
</svg>